								"method": "POST",
								"header": [],
								"body": {
									"mode": "raw",
									"raw": "{\n\t\"username\": \"app@app\",\n\t\"password\": \"app\"\n}",
									"options": {
										"raw": {
											"language": "json"
										}
									}
								},
								"url": {
									"raw": "{{url}}/user/jwt/create",
									"host": [
										"{{url}}"
									],
									"path": [
